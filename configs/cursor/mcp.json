{
  "mcpServers": {
    "MCP_DOCKER": {
      "command": "docker",
      "args": ["mcp", "gateway", "run"],
      "env": {
        "MCP_DOCKER": "true",
        "MCP_DOCKER_GATEWAY": "http://localhost:3000"
      }
    },
    "figma": {
      "command": "npx",
      "args": ["-y", "@figma/mcp-server-figma"],
    }
  }
}

